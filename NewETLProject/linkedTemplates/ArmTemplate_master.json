{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "NewETLProject"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorageLinkedServices_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLinkedServices'"
		},
		"RestService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newetlproject.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageLinkedServices_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newetlproject.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://newetlprojectKV.vault.azure.net/"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.kaggle.com/api/v1/datasets/download"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ameedahmed"
		},
		"RestaurantHttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/ameedahmed/E2EDataEngineeringProject/refs/heads/main/Restaurant%20reviews.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "NewETLProject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorageLinkedServices_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLinkedServices_accountKey')]"
					},
					"RestService1_password": {
						"value": "[parameters('RestService1_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageLinkedServices_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLinkedServices_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					},
					"RestaurantHttpServer1_properties_typeProperties_url": {
						"value": "[parameters('RestaurantHttpServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}